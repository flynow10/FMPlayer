{
  "name": "fmplayer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "vercel-dev": "vercel dev --listen 5173 --local-config ./vercel.local.json",
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "yarn lint:js; yarn lint:style",
    "lint:fix": "yarn lint:jsfix; yarn lint:stylefix",
    "lint:js": "eslint ./src ./api ./api-lib",
    "lint:jsfix": "yarn lint:js --fix",
    "lint:style": "stylelint \"./src/**/*.(css|sass|scss)\"",
    "lint:stylefix": "yarn lint:style --fix",
    "clean": "yarn clean:build; yarn clean:test",
    "clean:build": "rm -rf ./dist",
    "clean:test": "rm -rf ./coverage",
    "preview": "vite preview",
    "test": "jest",
    "test:coverage": "yarn test --collect-coverage",
    "postinstall": "prisma generate; patch-package",
    "prepare": "husky install"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
